<%= javascript_include_tag "typed.js"%>
<body class="homepage-body">
	<div class="enter-bx">
		<h1>Sign In</h1>
		  <%= form_tag "sign_in" do %>
		    <%= text_field_tag :login, "", placeholder: "Username or Email", class: "enter-field" %>
		    <%= password_field_tag :password, "", placeholder: "Password", class: "enter-field" %>
		    <%= submit_tag "Sign In", class: "button" %>
		  <% end %>
	</div>
</body>
